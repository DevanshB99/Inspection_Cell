  <!-- JOINT: Connect parent (world) to table - table positioned so top is at (0,0,0) -->
  <joint name="table_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="world"/>
    <child link="table_link"/>
  </joint>
  <!-- JOINT: Connect table to base_link (at table surface center, which is (0,0,0)) -->
  <joint name="table_to_base" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="table_link"/>
    <child link="base_link"/>
  </joint>
  <!-- JOINT: Connect base_link to turntable disc (30.25 inches left of center) -->
  <joint name="base_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.76835 0 0"/>
    <parent link="base_link"/>
    <child link="turntable_disc_link"/>
  </joint>
  <!-- JOINT 1: Turntable disc rotation (DOF 1) -->
